      <div class="validation"></div>
    </div>
    <!-- Lat Long -->
    <div><h5 style="color:white; padding-left: 15px;">Latitude & Longitude</h5></div>
    <div class="form-group col-sm-6">
        <input type="number " class="form-control" name="lat" id="lat" placeholder="Latitude" data-rule="" data-msg="Give Access to Location"/>
        <div class="validation"></div>
      </div>
      <div class="form-group col-sm-6">
        <input type="number " class="form-control" name="long"  id="long" placeholder="Longitude" data-rule="" data-msg="GIve Access to Location"/>
        <div class="validation"></div>
      </div>

<!-- Healthcard-pH -->
